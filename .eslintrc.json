{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": ["error", {"ignoreRestSiblings": true}],
        "no-useless-escape": 0,
        "no-redeclare": 0
    },
    "globals": {
        "process": true
    }
}